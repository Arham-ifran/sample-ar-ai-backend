{
  "name": "ar-ai",
  "version": "1.0.0",
  "description": "AR-AI is a web app that uses real-time face animation and advanced text-to-speech to create an immersive and human-like conversational AI experience.",
  "main": "src/index.js",
  "author": "Arham",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./src/index.js -i max",
    "dev": "nodemon ./src/index.js",
    "pm2": "pm2 start ./src/index.js --name=ar-ai",
    "pm2-restart": "pm2 restart ar-ai",
    "pm2-stop": "pm2 stop ar-ai",
    "pm2-list": "pm2 list",
    "pm2-delete": "pm2 delete ar-ai",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nodejs",
    "Arham",
    "AI",
    "Chat"
  ],
  "dependencies": {
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "download": "^8.0.0",
    "express": "^4.18.2",
    "express-bearer-token": "^2.4.0",
    "express-rate-limit": "^7.1.0",
    "express-validation": "^4.1.0",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.0",
    "google-auth-library": "^9.1.0",
    "http-status": "^1.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-simple": "^0.5.6",
    "mailgun.js": "^9.3.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mongoose": "^7.5.4",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2",
    "stripe": "^13.9.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
